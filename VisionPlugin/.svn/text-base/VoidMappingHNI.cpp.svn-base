template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid2dTypes, Vec2dTypes >;
#endif
#ifndef SOFA_DOUBLE
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec3fTypes, Vec3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec2fTypes, Vec2fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec1fTypes, Vec1fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec6fTypes, Vec6fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec3fTypes, ExtVec3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid3fTypes, Rigid3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid2fTypes, Rigid2fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid3fTypes, Vec3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid3fTypes, ExtVec3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid2fTypes, Vec2fTypes >;
#endif

#ifndef SOFA_FLOAT
#ifndef SOFA_DOUBLE
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec3dTypes, Vec3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec3fTypes, Vec3dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec2dTypes, Vec2fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec2fTypes, Vec2dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec1dTypes, Vec1fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec1fTypes, Vec1dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec6dTypes, Vec3fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec6dTypes, Vec6fTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec6fTypes, Vec6dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec6fTypes, Vec3dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid3fTypes, Rigid3dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Rigid3dTypes, Rigid3fTypes >;
