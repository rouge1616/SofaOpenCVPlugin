#define SOFA_COMPONENT_MAPPING_VoidMappingHNI_CPP
#include "VoidMappingHNI.inl"
#include <sofa/defaulttype/VecTypes.h>
#include <sofa/defaulttype/RigidTypes.h>
#include <sofa/core/ObjectFactory.h>

namespace sofa
{

namespace component
{

namespace mapping
{

using namespace sofa::defaulttype;
using namespace core;
using namespace core::behavior;


SOFA_DECL_CLASS(VoidMappingHNI)

// Register in the Factory
int VoidMappingHNIClass = core::RegisterObject("Special case of mapping where the child points are the same as the parent points")
#ifndef SOFA_FLOAT
        .add< VoidMappingHNI< Vec3dTypes, Vec3dTypes > >()
        .add< VoidMappingHNI< Vec2dTypes, Vec2dTypes > >()
        .add< VoidMappingHNI< Vec1dTypes, Vec1dTypes > >()
        .add< VoidMappingHNI< Vec6dTypes, Vec3dTypes > >()
        .add< VoidMappingHNI< Vec6dTypes, Vec6dTypes > >()
        .add< VoidMappingHNI< Rigid3dTypes, Rigid3dTypes > >()
        .add< VoidMappingHNI< Rigid2dTypes, Rigid2dTypes > >()
        .add< VoidMappingHNI< Vec3dTypes, ExtVec3fTypes > >()
        .add< VoidMappingHNI< Vec6dTypes, ExtVec3fTypes > >()
#endif
#ifndef SOFA_DOUBLE
        .add< VoidMappingHNI< Vec3fTypes, Vec3fTypes > >()
        .add< VoidMappingHNI< Vec2fTypes, Vec2fTypes > >()
        .add< VoidMappingHNI< Vec1fTypes, Vec1fTypes > >()
        .add< VoidMappingHNI< Vec6fTypes, Vec6fTypes > >()
        .add< VoidMappingHNI< Rigid3fTypes, Rigid3fTypes > >()
        .add< VoidMappingHNI< Rigid2fTypes, Rigid2fTypes > >()
        .add< VoidMappingHNI< Vec3fTypes, ExtVec3fTypes > >()
#endif
#ifndef SOFA_FLOAT
#ifndef SOFA_DOUBLE
        .add< VoidMappingHNI< Vec3fTypes, Vec3dTypes > >()
        .add< VoidMappingHNI< Vec3dTypes, Vec3fTypes > >()
        .add< VoidMappingHNI< Vec2fTypes, Vec2dTypes > >()
        .add< VoidMappingHNI< Vec2dTypes, Vec2fTypes > >()
        .add< VoidMappingHNI< Vec1fTypes, Vec1dTypes > >()
        .add< VoidMappingHNI< Vec1dTypes, Vec1fTypes > >()
        .add< VoidMappingHNI< Vec6fTypes, Vec6dTypes > >()
        .add< VoidMappingHNI< Vec6dTypes, Vec6fTypes > >()
        .add< VoidMappingHNI< Rigid3dTypes, Rigid3fTypes > >()
        .add< VoidMappingHNI< Rigid3fTypes, Rigid3dTypes > >()
        .add< VoidMappingHNI< Rigid2dTypes, Rigid2fTypes > >()
        .add< VoidMappingHNI< Rigid2fTypes, Rigid2dTypes > >()
#endif
#endif

// Rigid -> Vec
#ifndef SOFA_FLOAT
        .add< VoidMappingHNI< Rigid3dTypes, Vec3dTypes > >()
        .add< VoidMappingHNI< Rigid2dTypes, Vec2dTypes > >()
        .add< VoidMappingHNI< Rigid3dTypes, ExtVec3fTypes > >()
#endif
#ifndef SOFA_DOUBLE
        .add< VoidMappingHNI< Rigid3fTypes, Vec3fTypes > >()
        .add< VoidMappingHNI< Rigid2fTypes, Vec2fTypes > >()
        .add< VoidMappingHNI< Rigid3fTypes, ExtVec3fTypes > >()
#endif
        ;


#ifndef SOFA_FLOAT
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec3dTypes, Vec3dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec2dTypes, Vec2dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec1dTypes, Vec1dTypes >;
template class SOFA_BASE_MECHANICS_API VoidMappingHNI< Vec6dTypes, Vec3dTypes >;
