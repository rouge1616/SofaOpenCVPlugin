#include "SofaARCapsule.h"

SofaARCapsule::SofaARCapsule()
{

}

SofaARCapsule::SofaARCapsule(
    std::string img,
    int imgDepth,
    int imgNC,
    int imgWidth,
    int imgHeight,
    std::vector<float> currentPositionX,
    std::vector<float> currentPositionY,
    std::vector<float> currentPositionZ,
    std::vector<int> status,
    std::vector<double> currentQualities
    )
    :
    m_img(img),
    m_imgDepth(imgDepth),
    m_imgNC(imgNC),
    m_imgWidth(imgWidth),
    m_imgHeight(imgHeight),
    m_currentPositionX(currentPositionX),
    m_currentPositionY(currentPositionY),
    m_currentPositionZ(currentPositionZ),
    m_status(status),
    m_currentQualities(currentQualities)
{

}

SofaARCapsule::~SofaARCapsule()
{
}

// *** getter and setter

std::string SofaARCapsule::getImg()
{
  return m_img;
}

int SofaARCapsule::getImgDepth()
{
  return m_imgDepth;
}

int SofaARCapsule::getImgNC()
{
  return m_imgNC;
}

int SofaARCapsule::getImgWidth()
{
  return m_imgWidth;
}

int SofaARCapsule::getImgHeight()
{
  return m_imgHeight;
}

std::vector<float> SofaARCapsule::getCurrentPositionX()
{
  return m_currentPositionX;
}

std::vector<float> SofaARCapsule::getCurrentPositionY()
{
  return m_currentPositionY;
}

std::vector<float> SofaARCapsule::getCurrentPositionZ()
{
  return m_currentPositionZ;
}

std::vector<int> SofaARCapsule::getStatus()
{
  return m_status;
}

std::vector<double> SofaARCapsule::getCurrentQualities()
{
  return m_currentQualities;
}


